<resources xmlns:android="http://schemas.android.com/apk/res/android">
	<style name="video_merginLeft10dp">
	    <item name="android:layout_marginLeft">10dp</item>
	</style>
    <style name="video_btn" parent="video_merginLeft10dp">
        <item name="android:textSize">16.0dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_height">30dp</item>
         <item name="android:layout_width">80dp</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        
    </style>
	<style name="video_btn_small" parent="video_btn">
        <item name="android:textSize">15.0dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_height">30dp</item>
         <item name="android:layout_width">60dp</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
    </style>
    <style name="video_stat_icon_style">
        <item name="android:layout_width">26dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>
     <style name="video_stat_icon_style_port" >
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">26dp</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>
    <style name="video_stat_style">
        <item name="android:textSize">12.0dp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxWidth">50dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:singleLine">true</item>
    </style>
     <style name="video_edittext_style">
        <item name="android:textSize">20.0dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">#30FFFFFF</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>
    <style name="video_start_text">
        <item name="android:textSize">20.0dp</item>
         <item name="android:textColor">#FFFFFF</item>
         <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="video_player_btn_land" parent="ts12_white_ww_wh">
        <item name="android:background">#7F000000</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>
    <style name="video_player_btn_port" parent="ts12_white_ww_wh">
        <item name="android:background">#7F000000</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>
    <style name="video_danmu_option_select_btn" parent="ts14_white">
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:background">@drawable/video_btn_white_green_drawable</item>
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/video_btn_textcolor_white_green</item>
    </style>
</resources>
