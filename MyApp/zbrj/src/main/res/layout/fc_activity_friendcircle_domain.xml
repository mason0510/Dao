<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:oncon="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.lz.oncon.activity.friendcircle.DomianListView
        android:id="@+id/domian_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:cacheColorHint="#00000000"
        android:divider="#d7d6d4"
        android:dividerHeight="1px"
        android:listSelector="@android:color/transparent"
        android:orientation="vertical"
        android:scrollbars="none" />

    
     <com.lz.oncon.widget.TitleView
        android:id="@id/title"
        style="@style/title"
        oncon:centerValue="@string/fc_mydomain"
        oncon:leftImageSrc="@drawable/ic_back"
         />
    
</FrameLayout>